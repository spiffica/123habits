			<%= simple_form_for [@habit, tracker] do |f| %>
				<% unless tracker.marked? %>
					<%= f.collection_radio_buttons(:outcome,
						[["pass","pass"],["fail","fail"]], 
					  :first, :last,
						:item_wrapper_class => 'inline') do |outcome| %>
						<% outcome.label { image_tag("#{outcome.text}.png", 
						class: "click_pic_#{outcome.text}") + outcome.radio_button } %>
					<% end %>
					<%= f.submit "Submit", class: "hide btn btn-primary" %>
				<% end %>	
				<% if tracker.marked? %>
					<%= f.input :notes, as: :text %>
					<%= f.submit "Submit", id:"submit-notes", class: "btn btn-primary" %>
				<% end %>
			<% end %>